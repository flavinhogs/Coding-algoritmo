                <then>
                    <output expression="&quot;Certo, voc&#234; escolheu apenas banho.&quot;" newline="True"/>
                    <output expression="&quot;Fazendo espuma...&quot;" newline="True"/>
                    <output expression="nome &amp; &quot; limpinhe &lt;3&quot;" newline="True"/>
                </then>
                <else>
                    <output expression="&quot;Certo, voc&#234; escolheu banho e tosa.&quot;" newline="True"/>
                    <output expression="&quot;Tosando...&quot;" newline="True"/>
                    <output expression="&quot;Encaminhando &quot; &amp; nome &amp; &quot; para o banho.&quot;" newline="True"/>
                    <output expression="&quot;Fazendo espuma...&quot;" newline="True"/>
                    <output expression="nome &amp; &quot; limpinhe &lt;3&quot;" newline="True"/>
                </else>
            </if>
            <output expression="&quot;Encaminhando &quot; &amp; nome &amp; &quot; para secagem.&quot;" newline="True"/>
            <output expression="&quot;Secando &quot; &amp; nome &amp; &quot;...&quot;" newline="True"/>
            <output expression="nome &amp; &quot; sequinhe &lt;3&quot;" newline="True"/>
            <output expression="nome &amp; &quot; ser&#225; encaminhade para antipulga, coloca&#231;&#227;o de coleira e adere&#231;os :)&quot;" newline="True"/>
            <output expression="&quot;Agora que a beleza e cuidado de &quot; &amp; nome &amp; &quot; foram renovadas, elu ser&#225; encaminhade ao canil.&quot;" newline="True"/>
            <output expression="&quot;Finalizamos nosso servi&#231;o. &quot; &amp; nome &amp; &quot; est&#225; prontinhe para ser retirade. Obrigade pela prefer&#234;ncia &lt;3&quot;" newline="True"/>
