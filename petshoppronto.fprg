    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="nome" type="String" array="False" size=""/>
            <output expression="&quot;Seja bem-vinde ao Petshop, qual o nome de sue pet?&quot;" newline="True"/>
            <input variable="nome"/>
            <output expression="&quot;Que nome lindo, &quot; &amp; nome" newline="True"/>
            <output expression="&quot;Temos os seguintes servi&#231;os: apenas banho(Digite 1), banho e tosa(Digite 2).&quot;" newline="True"/>
            <declare name="ser" type="Integer" array="False" size=""/>
